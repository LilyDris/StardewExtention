{
  "manifest_version": 3,
  "name": "Stardew Extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/Animals_Icon.png"
  },
  "icons": {
    "16": "images/Animals_Icon.png",
    "32": "images/Animals_Icon.png",
    "48": "images/Animals_Icon.png",
    "128": "images/Animals_Icon.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/Animals_Icon.png",
      "32": "images/Animals_Icon.png",
      "48": "images/Animals_Icon.png"
    }
  },
    "description": "A Chrome extension for Stardew Valley fans.",
    "permissions": [
      "tabs"
    ]

  }
